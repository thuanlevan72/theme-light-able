<%- contentFor('HeaderCss') %>
  <!-- fileupload-custom css -->
  <link rel="stylesheet" href="/assets/css/plugins/dropzone.min.css">
  <%- contentFor('body') %>
    <%- include('../partials/breadcrumb.ejs', { 'breadcrumb_item' : 'Forms' , 'breadcrumb_item_active' : 'File Upload' }) %>
      <!-- [ Main Content ] start -->
      <div class="row">
        <!-- [ file-upload ] start -->
        <div class="col-sm-12">
          <div class="card">
            <div class="card-header">
              <h5>File Upload</h5>
            </div>
            <div class="card-body">
              <form action="../assets/json/file-upload.php" class="dropzone">
                <div class="fallback">
                  <input name="file" type="file" multiple>
                </div>
              </form>
              <div class="text-center m-t-20">
                <button class="btn btn-primary">Upload Now</button>
              </div>
            </div>
          </div>
        </div>
        <!-- [ file-upload ] end -->
      </div>
      <!-- [ Main Content ] end -->

      <%- contentFor('FooterJs') %>
        <!-- file-upload Js -->
        <script src="/assets/js/plugins/dropzone-amd-module.min.js"></script>